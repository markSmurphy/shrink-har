{
  "name": "shrink-har",
  "version": "1.0.3",
  "description": "HTTP Archive files can be large when they contain multiple response bodies. If this content is superfluous to your debugging needs then `shrink-har` allows you create smaller copies of .har files with the response body content stripped out.",
  "main": "shrink-har.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "private": false,
  "preferGlobal": true,
  "bin": {
    "shrink-har": "./shrink-har.js"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markSmurphy/shrink-har.git"
  },
  "bugs": {
    "url": "https://github.com/MarkSMurphy/shrink-har/issues"
  },
  "keywords": [
    ".har",
    "HAR",
    "HTTP",
    "HTTPS",
    "archive",
    "shrink",
    "reduce",
    "remove",
    "delete",
    "content",
    "response",
    "body",
    "dev tools"
  ],
